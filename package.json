{
	"name": "mrmixer-skeleton",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma": "prisma generate && prisma migrate dev --name init --preview-feature",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --name init --preview-feature",
		"prisma:studio": "prisma studio",
		"prisma:seed": "ts-node prisma/seed.ts",
		"prisma:reset": "prisma migrate reset --preview-feature",
		"prisma:delete": "prisma migrate reset --preview-feature && rm -rf ./prisma/migrations && rm -rf ./prisma/dev.db",
		"prisma:deploy": "prisma migrate deploy --preview-feature",
		"prisma:save": "prisma migrate save --experimental",
		"prisma:up": "prisma migrate up --experimental",
		"prisma:down": "prisma migrate down --experimental",
		"prisma:generate:watch": "prisma generate --watch"
	},
	"devDependencies": {
		"@floating-ui/dom": "1.5.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-netlify": "2.0.8",
		"@sveltejs/kit": "1.24.0",
		"@tailwindcss/forms": "0.5.6",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "20.5.9",
		"@typescript-eslint/eslint-plugin": "6.5.0",
		"@typescript-eslint/parser": "6.5.0",
		"autoprefixer": "10.4.15",
		"eslint": "8.48.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.33.0",
		"postcss": "8.4.29",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"prisma": "5.2.0",
		"svelte": "4.2.0",
		"svelte-check": "3.5.1",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.4.9",
		"vite-plugin-tailwind-purgecss": "^0.1.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "5.2.0",
		"contentful": "10.5.1",
		"pnpm": "8.8.0"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}
